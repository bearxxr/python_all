{
    "window.zoomLevel": 1,
    "editor.fontSize": 12.2,
    "C_Cpp.updateChannel": "Insiders",
    "terminal.integrated.fontSize": 12,
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontFamily": "JetBrains Mono",
    // "editor.lineHeight":"22", //行高
    "files.autoSave": "afterDelay",

    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#e41fda7e",   //选中高亮
    },
    "editor.tokenColorCustomizations":{
        "comments": "#ffae00",      //注释
        "keywords": "#0f0", 			// 关键字
        "variables": "#FFD700", 			// 变量名
        "strings": "#00FFFF", 		// 字符串
        // "functions": "#5bfc63c9", 	// 函数名
        "numbers": "#AE81FF",			// 数字
        "types": "#FF0000",//类型定义颜色
        "textMateRules": [
                            {
                                "scope": "meta.function.python",
                                "settings": {
                                    "foreground": "#FFC0CB"
                                }
                                                },
                            {
                                "scope": "meta.function-call.python",
                                "settings": {
                                    "foreground": "#0f0"
                                }
                                                },
                            {
                                "scope": "keyword.control.flow.python",
                                "settings": {
                                    "foreground": "#FFC0CB"
                                }
                            },
                            {
                                "scope": "meta.attribute.python",
                                "settings": {
                                    "foreground": "#00FFFF"
                                }
                            },
                            {
                                "scope": "keyword.control.import.python",
                                "settings": {
                                    "foreground": "#FFC0CB"
                                }
                            },
                            {
                                "scope": "keyword.operator.comparison.python",
                                "settings": {
                                    "foreground": "#AE81FF"
                                }
                            },
                            {
                                "scope": "meta.member.access.python",
                                "settings": {
                                    "foreground": "#FFC0CB"
                                }
                            },
                            {
                                "scope": "meta.function-call.arguments.python",
                                "settings": {
                                    "foreground": "#FFC0CB"
                                }
                            },
                            {
                                "scope": "meta.indexed-name.python",
                                "settings": {
                                    "foreground": "#ffee00"
                                }
                            },
                            {
                                "scope": "entity.name.type.class.python",
                                "settings": {
                                    "foreground": "#e02323"
                                }
                            }
    ]
    },
    "editor.fontLigatures": true,
    
    "editor.fontWeight": "500",
    "terminal.integrated.fontFamily": "Menlo for Powerline",
    "workbench.settings.editor": "json",
    "workbench.iconTheme": "material-icon-theme",
    "python.showStartPage": false,
}
